<div class="form-data">
    <form action="/courses/${course._id}/update?method=PUT" method="POST">
        <label for="txtTitle">Title</label>
    <input type="text" name="title" id="txtTitle" autofocus required value="course.title">
    
    <label for="txtDescription">Description</label>
    <textarea type="email" name="description" id="txtDescription" required value="course.description">

    <label for="txtMaxStudents">Max # Of Students</label>
    <input type="number" name="name" pattern="[0-9]" id="txtMaxStudents" required value="course.maxStudents">

    <label for="txtCost">Cost</label>
    <input type="number" name="email" id="txtCost" required value="course.cost">

    <button type="submit">Save</button>

    </form>
</div>