<div class="form-data">
    <form action="/subscribers/${subscriber._id}/update?method=PUT" method="POST">
    <label for="txtName">Name</label>
    <input type="text" name="name" id="txtName" autofocus required autocomplete="name" value="<%subscriber.name%>">
    
    <label for="txtEmail">Email Address</label>
    <input type="email" name="email" id="txtEmail" required autocomplete="email" value="<%subscriber.email%>">

    <label for="txtzipCode">Zip Code</label>
    <input type="text" name="name" pattern="[0-9]{5}" id="txtName" required autocomplete="postal-code" value="<%subscriber.zipCode%>">

    <button type="submit">Save</button>

    </form>
</div>